create table Category(
	id serial primary key,
	section varchar(255),
	backnames_id  integer references Backnames(id)
)


create table Address(
	name varchar(255),
	place varchar(255),
	category_id  integer references Category(id),
	backnames_id  integer references Backnames(id)
)

create table Backnames(
	id serial primary key,
	callback varchar(255)
)

